   <script>
    (function() {
        var xtHeader = $(document.getElementById('xt-header'));
        var searchWrap = xtHeader.find('.header_container_search');
        $('#xt-header-search-input').on({
            focus: function() {
                searchWrap.addClass('focus');
            },
            blur: function() {
                searchWrap.removeClass('focus');
            }
        });

        $.ajax({
            url: '/header_ajax',
            dataType: 'json'
        }).done(function(response) {
            xtHeader.find('.loader').remove();
            var msgHTML = '';
            if (response.notification.system.unread) {
                msgHTML = '&lt;i class="msg_count"&gt;' + response.notification.system.unread + '&lt;/i&gt;';
            }
            var HTML = '&lt;div id="header_container_msg" class="header_container_msg fl"&gt;' +
                '&lt;a title="我的消息" href="' + response.notification.system.redirect + '"&gt;' +
                    msgHTML +
                '&lt;/a&gt;' +
            '&lt;/div&gt;' +
            '&lt;div class="header_container_avatar fl" id="header_container_avatar"&gt;' +
                '&lt;a href="/dashboard/"&gt;' +
                    '&lt;img src="' + response.user_image + '"&gt;' +
                '&lt;/a&gt;' +
            '&lt;/div&gt;';
            $(HTML).insertAfter(searchWrap);
            $('#header_container_avatar').popup({
                hoverable: true,
                inline: true,
                position: 'bottom center',
                html: '&lt;div class="header_container_popup"&gt;&lt;p&gt;&lt;a href="/dashboard"&gt;&lt;span&gt;g&lt;/span&gt;我的主页&lt;/a&gt;&lt;/p&gt;&lt;p&gt;&lt;a href="/dashboard/message/" data-click="onClick" data-block="GLOBLNAV" data-description="TOMESSAGE"&gt;&lt;span&gt;c&lt;/span&gt;消息提醒&lt;/a&gt;&lt;/p&gt;&lt;p&gt;&lt;a href="/dashboard/account/" data-click="onClick" data-block="GLOBLNAV" data-description="TOACCOUNT"&gt;&lt;span class="icon-header-account"&gt;&lt;em&gt;&lt;/em&gt;&lt;/span&gt;账号信息&lt;/a&gt;&lt;/p&gt;&lt;p&gt;&lt;a href="/logout" data-click="onClick" data-block="GLOBLNAV" data-description="TOEXIT"&gt;&lt;span&gt;a&lt;/span&gt;退出&lt;/a&gt;&lt;/p&gt;&lt;/div&gt;',
                delay: {
                    show: 50,
                    hide: 100
                },
                preserve: true
            });
        }).fail(function(xhr) {
            console.error(xhr, 'header 用户信息加载失败');
        });
    })();
   </script>
